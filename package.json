{
  "name": "autolink-js",
  "version": "1.0.2",
  "description": "Tiny little tool to find URLs in a string of text and hyperlink them",
  "main": "autolink-min.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "bundle": "coffee --compile --map --transpile autolink.coffee && minify autolink.js --out-file autolink-min.js",
    "test": "open test/suite.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryanwoods/autolink-js.git"
  },
  "author": "Bryan Woods",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bryanwoods/autolink-js/issues"
  },
  "homepage": "https://github.com/bryanwoods/autolink-js#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-preset-env": "^1.6.0",
    "coffeescript": "^2.0.0"
  }
}
